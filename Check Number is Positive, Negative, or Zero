import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        if(num<1)
        {
            if(num==0)
            {
                System.out.println("zero");
            }
            else
            {
             System.out.println("negative");  
            }
        }
        else
        {
            System.out.println("positive");
        }
    }
}

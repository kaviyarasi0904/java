import java.util.*;
public class main{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int sum=0;
        int temp=num;
        
        while(temp>0)
        {
            int rem=temp%10;
            sum=rem+sum;
            temp=temp/10;
        }
        if((num%sum)==0)
        {
            System.out.println("Harshad Number");
        }
        else{
            System.out.println("Not Harshad Number");
        }
    }
}

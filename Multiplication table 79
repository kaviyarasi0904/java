import java.util.*;
public class Main{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int num=sc.nextInt();
        if(1<=num && num<=9)
        {
            for(int i=1;i<=num;i++)
            {
               System.out.println(num +" x " + i + " = " + (num*i)) ;
            }
        }
        else
        {
            System.out.println("Invalid Input");
        }
    }
}

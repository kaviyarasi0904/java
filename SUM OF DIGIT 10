import java.util.*;
public class Main{
    public static void main(String[] args)
    {
      Scanner sc=new Scanner(System.in);
      int num=sc.nextInt();
      if(num>=100)
      {
          int temp=0;
          while(num>0)
          {
              int rem=num%10;
              temp=rem+temp;
              num=num/10;  
          }
          System.out.println("Sum of digit is "+temp);
      }
        else
        {
            System.out.println("Invalid Input");
        }

    }
    
}
